/*
 * 	[문제-23] 구구단 2단에서부터 9단까지 모두 출력하시오
 * 
 * 	
 */
public class Exam_24 {

	public static void main(String[] args) {
		for(int dan=2; dan <=9 ; ++dan) {
			for (int i=1; i<=9; ++i) {
				for(int j=2; i<=5; ++j) {
					System.out.printf("%d x %d = %2d\t", j, i, (j*i));
				}
				System.out.println();
				
			
			}
		}
		
		
		
		
	}

